#: myvenv/lib/python3.12/site-packages/django/contrib/postgres/apps.py:54
msgid "PostgreSQL extensions"
msgstr "PostgreSQL-Erweiterungen"

#: myvenv/lib/python3.12/site-packages/django/contrib/postgres/fields/array.py:21
#: myvenv/lib/python3.12/site-packages/django/contrib/postgres/forms/array.py:17
#: myvenv/lib/python3.12/site-packages/django/contrib/postgres/forms/array.py:185
#, python-format
msgid "Item %(nth)s in the array did not validate:"
msgstr "Element %(nth)s im Array konnte nicht validiert werden:"

#: myvenv/lib/python3.12/site-packages/django/contrib/postgres/fields/array.py:22
msgid "Nested arrays must have the same length."
msgstr "Verschachtelte Arrays müssen die gleiche Länge haben."

#: myvenv/lib/python3.12/site-packages/django/contrib/postgres/fields/hstore.py:15
msgid "Map of strings to strings/nulls"
msgstr "Zuordnung von Zeichenketten zu Zeichenketten/NULLs"

#: myvenv/lib/python3.12/site-packages/django/contrib/postgres/fields/hstore.py:17
#, python-format
msgid "The value of “%(key)s” is not a string or null."
msgstr "Der Wert für „%(key)s“ ist keine Zeichenkette oder NULL."

#: myvenv/lib/python3.12/site-packages/django/contrib/postgres/forms/hstore.py:17
msgid "Could not load JSON data."
msgstr "Konnte JSON-Daten nicht laden."

#: myvenv/lib/python3.12/site-packages/django/contrib/postgres/forms/hstore.py:18
msgid "Input must be a JSON dictionary."
msgstr "Eingabe muss ein JSON-Dictionary sein."

#: myvenv/lib/python3.12/site-packages/django/contrib/postgres/forms/ranges.py:42
msgid "Enter two valid values."
msgstr "Bitte zwei gültige Werte eingeben."

#: myvenv/lib/python3.12/site-packages/django/contrib/postgres/forms/ranges.py:44
msgid "The start of the range must not exceed the end of the range."
msgstr "Der Anfang des Wertbereichs darf nicht das Ende überschreiten."

#: myvenv/lib/python3.12/site-packages/django/contrib/postgres/forms/ranges.py:99
msgid "Enter two whole numbers."
msgstr "Bitte zwei ganze Zahlen eingeben."

#: myvenv/lib/python3.12/site-packages/django/contrib/postgres/forms/ranges.py:105
msgid "Enter two numbers."
msgstr "Bitte zwei Zahlen eingeben."

#: myvenv/lib/python3.12/site-packages/django/contrib/postgres/forms/ranges.py:111
msgid "Enter two valid date/times."
msgstr "Bitte zwei gültige Datum/Uhrzeit-Werte eingeben."

#: myvenv/lib/python3.12/site-packages/django/contrib/postgres/forms/ranges.py:117
msgid "Enter two valid dates."
msgstr "Bitte zwei gültige Kalenderdaten eingeben."

#: myvenv/lib/python3.12/site-packages/django/contrib/postgres/validators.py:15
#, python-format
msgid ""
"List contains %(show_value)d item, it should contain no more than "
"%(limit_value)d."
msgid_plural ""
"List contains %(show_value)d items, it should contain no more than "
"%(limit_value)d."
msgstr[0] ""
"Liste enthält %(show_value)d Element, es sollte aber nicht mehr als "
"%(limit_value)d enthalten."
msgstr[1] ""
"Liste enthält %(show_value)d Elemente, es sollte aber nicht mehr als "
"%(limit_value)d enthalten."

#: myvenv/lib/python3.12/site-packages/django/contrib/postgres/validators.py:25
#, python-format
msgid ""
"List contains %(show_value)d item, it should contain no fewer than "
"%(limit_value)d."
msgid_plural ""
"List contains %(show_value)d items, it should contain no fewer than "
"%(limit_value)d."
msgstr[0] ""
"Liste enthält %(show_value)d Element, es sollte aber nicht weniger als "
"%(limit_value)d enthalten."
msgstr[1] ""
"Liste enthält %(show_value)d Elemente, es sollte aber nicht weniger als "
"%(limit_value)d enthalten."

#: myvenv/lib/python3.12/site-packages/django/contrib/postgres/validators.py:38
#, python-format
msgid "Some keys were missing: %(keys)s"
msgstr "Einige Werte fehlen: %(keys)s"

#: myvenv/lib/python3.12/site-packages/django/contrib/postgres/validators.py:39
#, python-format
msgid "Some unknown keys were provided: %(keys)s"
msgstr "Einige unbekannte Werte wurden eingegeben: %(keys)s"

#: myvenv/lib/python3.12/site-packages/django/contrib/postgres/validators.py:81
#, python-format
msgid ""
"Ensure that the upper bound of the range is not greater than %(limit_value)s."
msgstr ""
"Bitte sicherstellen, dass die obere Grenze des Bereichs nicht größer als "
"%(limit_value)s ist."

#: myvenv/lib/python3.12/site-packages/django/contrib/postgres/validators.py:90
#, python-format
msgid ""
"Ensure that the lower bound of the range is not less than %(limit_value)s."
msgstr ""
"Bitte sicherstellen, dass die untere Grenze des Bereichs nicht kleiner als "
"%(limit_value)s ist."
